        <section class="SectionContainerContact1">
            <!-- Section 1 -->

            <div>

                <div class="ContainerContactSection1">



                    <form method="post" action="" id="CadreFormContact">


                        <fieldset>

                            <legend class="text-center"> Informations personnelles </legend>

                            <div class="TextForm">
                                <label for="SurName"> Nom </label> <p> <input type="text" name="SurName" id="SurName" size="25" maxlength="30" value="@Request.Form["Surname"]" /> </p>
                                <label for="Name"> Prénom </label> <p> <input type="text" name="Name" id="Name" size="25" maxlength="30" value="@Request.Form["Name"]" /> </p>
                                <label for="NumStreet"> Numéro rue </label> <p> <input type="number" name="NumStreet" size="25" maxlength="30" style="padding-right:42px;" value="@Request.Form["NumStreet"]" /> </p>
                                <label for="NameStreet"> Nom rue  </label> <p> <input type="text" name="NameStreet" id="NameStreet" size="25" maxlength="30" value="@Request.Form["NameStreet"]" />  </p>
                                <label for="City"> Ville  </label> <p> <input type="text" name="City" id="City" size="25" maxlength="30" value="@Request.Form["City"]" /> </p>
                                <label for="NumTel"> Téléphone fixe ou portable  </label> <p> <input type="tel" name="NumTel" id="NumTel" size="25" maxlength="10" placeholder="+33..." value="@Request.Form["NumTel"]" /> </p>
                                <label for="Mail"> E-mail </label> <p> <input type="email" name="Email" id="Mail" size="25" maxlength="50" value="@Request.Form["Email"]" /> </p>

                                <label for="Comment"> Puis-je vous aider ? </label> <p><textarea name="Comment" id="Comment" rows="10" cols="40" required>  </textarea></p>

                            </div>

                        </fieldset>

                        <input type="submit" name="SendContact" id="SendContact" alt="Send" value="Envoyez" />

                    </form>

                </div>
            </div>

        </section>
