            <h3 class="text-center"> Informations Contact </h3>

            <div class="borderContact">


                @using (Html.BeginForm("Contact", "User", FormMethod.Post, new { enctype = "multipart/form-date" }))
                {

                    <label for="Subject"> Sujet : </label>
                    <p>

                        @Html.TextBoxFor(model => model.SUBJECT, new { required = "", size = "25", maxlength = "30", })

                        @Html.ValidationMessageFor(Model => Model.SUBJECT)

                    </p>

                    <hr class="alert-danger hr" />

                    <label for="Comment"> Description </label>
                    <p>
                        @Html.TextAreaFor(model => model.COMMENT, new { required = "", placeholder = "Nom : \n Prénom : \n Adresse e-mail : \n Ville : \n Numéro de téléphone : \n Description :", rows = "10", cols = "150", })

                        @Html.ValidationMessageFor(model => model.COMMENT)

                    </p>

                    <br />
                    @ViewBag.message

                    <p>
                        <input type="submit" value="Envoyer" id="BoutonEnvoyez" />
                    </p>
                }

